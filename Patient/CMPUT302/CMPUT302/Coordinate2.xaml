<Window x:Class="CMPUT302.Coordinate2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Coordinate2" Height="600" Width="800">
    <Grid>
        <Image Name="videoImage"></Image>
        <Canvas Background="Transparent">
            <Button Content="Set Coordinate" HorizontalAlignment="Left" x:Name="btnsetTilt" Click="setCoordinate2Click" Width="200" Height="60" Canvas.Left="300" Canvas.Top="270"/>
            <Rectangle Fill="Red" Height="50" Width="50" Name="target" Stroke="White" Canvas.Left="0" Canvas.Top="0"/>
        </Canvas>
        <Grid HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
